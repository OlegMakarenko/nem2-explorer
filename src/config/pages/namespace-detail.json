{
    "path": "/namespace/:namespaceId",
    "name": "namespace-detail",
    "meta": {
        "group": "detail-page",
        "keepAliveGoTo": []
    },
    "props": {
        "layout": "flex",
        "storeNamespaces": [],
        "initActions": ["namespace/fetchNamespaceInfo"],
        "schema": [
            {
                "layoutOptions": "full-width",
                "type": "CardTable",
                "title": "Namespace Detail",
                "managerGetter": "namespace/info",
                "errorMessage": "Namespace does not exist.",
                "pagination": "none",
                "fields": [
                    "owneraddress",
                    "namespaceName",
                    "namespaceId",
                    "registrationType",
                    "status",
                    "aliasType",
                    "aliasAddress",
                    "aliasMosaic",
                    "startHeight",
                    "expiredInBlock",
                    "beforeEndHeight",
                    "endHeight"
                ]
            },
            {
                "layoutOptions": "full-width",
                "type": "CardTable",
                "title": "Namespace Level",
                "managerGetter": "namespace/namespaceLevel",
                "pagination": "none",
                "fields": [
                    "name",
                    "namespaceId",
                    "parentId"
                ]
            },
            {
                "layoutOptions": "full-width",
                "type": "CardTable",
                "title": "Metadata Entries",
                "managerGetter": "namespace/metadatas",
                "pagination": "client",
                "pageSize": 5,
                "fields": [
                    "metadataId",
                    "scopedMetadataKey",
                    "targetId",
                    "senderAddress",
                    "targetAddress",
                    "metadataValue"
                ]
            }
        ]
    }
}