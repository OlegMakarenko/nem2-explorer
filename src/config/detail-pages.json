{
    "pages": [
        {
            "path": "/block/:height",
            "name": "block-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Block Detail",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "Pagination",
                                "canFetchPrevious": true,
                                "canFetchNext": true,
                                "nextPageAction": "block/nextBlock",
                                "previousPageAction": "block/previousBlock"
                            }
                        ],
                        "body": "TableInfoView",
                        "fields": [
                            "height",
                            "date",
                            "totalFee",
                            "difficulty",
                            "feeMultiplier",
                            "transactions",
                            "harvester",
                            "blockHash",
                            "proofGamma",
                            "proofScalar",
                            "proofVarificationHash"
                        ],
                        "pagination": false,
                        "managerGetter": "block/info",
                        "errorMessage": "Block does not exist."
                    },
                    {
                        "type": "Card",
                        "title": "Block Transactions",
                        "fullWidth": true,
                        "body": "TableListView",
                        "header": [
                            {
                                "type": "filter",
                                "filterGetter": "block/transactions"
                            }
                        ],
                        "fields": [
                            "deadline",
                            "transactionId",
                            "transactionHash",
                            "type"
                        ],
                        "managerGetter": "block/transactions",
                        "timelinePagination": true
                    },
                    {
                        "type": "Card",
                        "title": "Balance Change Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceChangeReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": false,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Balance Transfer Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceTransferReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Inflation Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/inflationReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Artifact Expiry Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/artifactExpiryReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Resolution Statement",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/resolutionStatement",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        },
        {
            "path": "/mosaic/:mosaicId",
            "name": "mosaic-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Mosaic Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "mosaicId",
                            "mosaicAliasName",
                            "divisibility",
                            "address",
                            "supply",
                            "relativeAmount",
                            "revision",
                            "startHeight",
                            "duration",
                            "supplyMutable",
                            "transferable",
                            "restrictable"
                        ],
                        "managerGetter": "mosaic/info",
                        "pagination": false,
                        "errorMessage": "Mosaic does not exist."
                    },
                    {
                        "type": "Card",
                        "title": "Mosaic Restriction",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "fields": [
                            "compositeHash",
                            "entryType"
                        ],
                        "managerGetter": "mosaic/restrictions",
                        "pagination": false,
                        "errorMessage": "Mosaic does not exist."
                    },
                    {
                        "type": "Card",
                        "title": "Mosaic Restriction List",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "restrictionKey",
                            "restrictionType",
                            "restrictionValue"
                        ],
                        "managerGetter": "mosaic/restrictions",
                        "dataGetter": "mosaic/getMosaicRestrictionList",
                        "pagination": false,
                        "errorMessage": "Mosaic global restrictions are not available."
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "targetId",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "mosaic/metadatas",
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        },
        {
            "path": "/namespace/:namespaceId",
            "name": "namespace-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Namespace Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "owneraddress",
                            "namespaceName",
                            "namespaceId",
                            "registrationType",
                            "status",
                            "aliasType",
                            "aliasAddress",
                            "aliasMosaic",
                            "startHeight",
                            "expiredInBlock",
                            "beforeEndHeight",
                            "endHeight"
                        ],
                        "managerGetter": "namespace/info",
                        "pagination": false,
                        "errorMessage": "Namespace does not exist."
                    },
                    {
                        "type": "Card",
                        "title": "Namespace Level",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "name",
                            "namespaceId",
                            "parentId"
                        ],
                        "managerGetter": "namespace/namespaceLevel",
                        "pagination": false
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "targetId",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "namespace/metadatas",
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        }
    ]
}